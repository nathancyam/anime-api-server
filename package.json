{
  "name": "Anime_Episode_App",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "start": "NODE_ENV=production; node ./src/server.js --config=./config.json",
    "test": "NODE_ENV=testing;PORT=3333 mocha --config=./config.test.json './tests/**/*Test.js'",
    "test:tap": "mocha -R tap -- './tests/**/*Test.js'",
    "unittest": "NODE_ENV=testing;PORT=3333 mocha --config=/tmp/config.json './tests/unit/**/*Test.js'",
    "test:watch": "NODE_ENV=testing;PORT=3333 mocha --watch --reporter dot './tests/unit/**/*Test.js'",
    "unittest:watch": "NODE_ENV=testing;PORT=3333 mocha --watch --reporter dot './tests/unit/**/*Test.js'",
    "ci:test": "NODE_ENV=testing_ci;PORT=3333 ./node_modules/.bin/istanbul cover npm run test:tap > test.tap && ./node_modules/.bin/istanbul report clover"
  },
  "dependencies": {
    "async": "*",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.15.0",
    "cheerio": "^0.16.0",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.1",
    "express": "4.*",
    "express-session": "^1.13.0",
    "feedparser": "^2.2.0",
    "fuse.js": "^3.0.0",
    "google": "*",
    "ioredis": "^1.15.1",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^2.4.1",
    "minimist": "^1.2.0",
    "moment": "^2.12.0",
    "moment-duration-format": "^1.3.0",
    "mongoose": "*",
    "node-uuid": "^1.4.7",
    "nyaatorrents": "*",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "q": "*",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "request": "^2.34.0",
    "socket.io": "^1.4.5",
    "web-push": "^3.2.2",
    "xml2js": "*"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "flow-bin": "0.49.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "sinon": "*",
    "supertest": "^1.2.0",
    "winston": "^2.2.0"
  }
